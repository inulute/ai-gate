{
  "name": "ai-gate",
  "productName": "AI Gate",
  "version": "4.0.2",
  "description": "Access all your AI tools seamlessly in one unified desktop application",
  "main": "dist-electron/main.js",
  "author": {
    "name": "inulute",
    "email": "connect@inulute.com",
    "url": "https://inulute.com"
  },
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/inulute/ai-gate#readme",
  "bugs": {
    "url": "https://github.com/inulute/ai-gate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inulute/ai-gate.git"
  },
  "keywords": [
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "perplexity",
    "ai-tools",
    "desktop-app",
    "electron"
  ],
  "scripts": {
    "start": "electron .",
    "start-minimized": "electron . --hidden",
    "dev": "vite",
    "build": "npx tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:dev": "npx tsc -p tsconfig.electron.json && npx tsc -p tsconfig.preload.json && cross-env NODE_ENV=development ELECTRON=true concurrently \"vite\" \"electron .\"",
    "electron:build": "npx tsc -p tsconfig.electron.json && npx tsc -p tsconfig.preload.json && cross-env NODE_ENV=production ELECTRON=true vite build && electron-builder",
    "electron:preview": "npx tsc -p tsconfig.electron.json && npx tsc -p tsconfig.preload.json && npx cross-env NODE_ENV=production ELECTRON=true vite build && cross-env NODE_ENV=production electron .",
    "electron:preview:dev": "cross-env NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "package-win": "electron-builder --win",
    "package-mac": "electron-builder --mac",
    "package-linux": "electron-builder --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "build": {
    "appId": "com.inulute.aigate",
    "productName": "AI Gate",
    "directories": {
      "output": "release-builds"
    },
    "asar": true,
    "compression": "maximum",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "icons/**/*",
      "!**/*.map"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "./icons/mac/favicon.icns",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64", "universal"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64", "universal"]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "./icons/win/favicon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "AI Gate",
      "runAfterFinish": true,
      "installerIcon": "./icons/win/favicon.ico",
      "uninstallerIcon": "./icons/win/favicon.ico",
      "installerHeaderIcon": "./icons/win/favicon.ico",
      "artifactName": "${productName}-Setup-${version}-${arch}.${ext}",
      "differentialPackage": true
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "./icons/png/favicon.png",
      "category": "Utility",
      "synopsis": "AI Gate - Access AI tools seamlessly",
      "desktop": {
        "StartupWMClass": "ai-gate"
      },
      "artifactName": "${productName}-${version}.${ext}"
    },
    "appImage": {
      "license": "LICENSE",
      "category": "Utility"
    },
    "deb": {
      "depends": [
        "libnotify4",
        "libxtst6",
        "libnss3"
      ],
      "compression": "xz"
    },
    "publish": {
      "provider": "github",
      "owner": "inulute",
      "repo": "ai-gate"
    }
  }
}